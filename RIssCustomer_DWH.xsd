<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="ExactFile" type="ExactFileType"/>
  <xs:complexType name="ExactFileType">
    <xs:sequence>
      <xs:element name="FileId" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Exact's internal file identification</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:totalDigits value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FileType" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>File type</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
            <xs:enumeration value="RIssCustomer"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="InstId" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>ID of the institution</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:totalDigits value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LogicalNetwork" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Partner identifier at all system levels</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FileDateTime" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Timestamp when the file was created, format is yyyyMMddHHmmss</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="14"/>
            <xs:maxLength value="14"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="CutOffDate" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Date when the file was created, format is yyyyMMdd</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="8"/>
            <xs:maxLength value="8"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FileName" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Name of the file, in format RIssCustomer_DWH_****_yyyyMMdd_HHmmss.xml</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="128"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FileSequenceNumber" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>This file is divided after every n records, where n is usually equal to 10000. So if there are 70000 records, there will be 7 files. This field denotes what is the current file's sequence number, i.e. in this case number between 1 and 7</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:totalDigits value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FileMode" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>File status.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1"/>
            <xs:enumeration value="T">
              <xs:annotation>
                <xs:documentation>Test</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="P">
              <xs:annotation>
                <xs:documentation>Production</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Source" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Transaction source</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="TotalFileCount" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Total number of files. This file is divided after every n records, where n is usually equal to 10000. So if there are 70000 records, there will be 7 files and total number will be 7. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:totalDigits value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="TotalRecordsCount" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Total number of records. This file is divided after every n records, where n is usually equal to 10000. So if there are 69500 records, there will be 7 files and total number of records will be 69500 in each of the 7 files. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:totalDigits value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="RecordsCount" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Number of records in the current file</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:totalDigits value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Records" type="ArrayOfRIssCustomer" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ArrayOfRIssCustomer">
    <xs:sequence>
      <xs:element name="RIssCustomer" type="RIssCustomerType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="RIssCustomerType">
    <xs:sequence>
      <xs:element name="Addresses" type="ArrayOfAddress" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Addresses used by the customer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Cards" type="ArrayOfCard" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Cards used by the customer</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CardAccounts" type="ArrayOfCardAccount" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>CardAccounts owned by the customer</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="CustomerId" use="required">
      <xs:annotation>
        <xs:documentation>Partner provided customer identifier</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CustomerType" use="required">
      <xs:annotation>
        <xs:documentation>Customer type</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:int">
          <xs:enumeration value="1"/>
          <xs:enumeration value="2"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Name1" use="required">
      <xs:annotation>
        <xs:documentation>Person first name / Company short name</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Name2" use="required">
      <xs:annotation>
        <xs:documentation>Person last name / Company long name</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="300"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Gender">
      <xs:annotation>
        <xs:documentation>Gender of the customer when customer is a person</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="1"/>
          <xs:enumeration value="M">
            <xs:annotation>
              <xs:documentation>Male</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="F">
            <xs:annotation>
              <xs:documentation>Female</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Birthdate">
      <xs:annotation>
        <xs:documentation>Birthdate or company establishment date</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="8"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Title">
      <xs:annotation>
        <xs:documentation>Person's title</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="50"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="ResidentialStatus">
      <xs:annotation>
        <xs:documentation>Customer's residential status</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="1"/>
          <xs:enumeration value=""/>
          <xs:enumeration value="D">
            <xs:annotation>
              <xs:documentation>Resident</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="N">
            <xs:annotation>
              <xs:documentation>NonResident</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="X">
            <xs:annotation>
              <xs:documentation>Uncategorized</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Email">
      <xs:annotation>
        <xs:documentation>Customer main e-mail address</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="TelStat">
      <xs:annotation>
        <xs:documentation>Customer phone number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="TelMobile">
      <xs:annotation>
        <xs:documentation>Customer mobile number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Password">
      <xs:annotation>
        <xs:documentation>Client’s password for identification over phone</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="300"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Code1">
      <xs:annotation>
        <xs:documentation>State identification number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="50"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Code2">
      <xs:annotation>
        <xs:documentation>Tax identification number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="50"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="UserData">
      <xs:annotation>
        <xs:documentation>Bank specific data fields</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="512"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="ArrayOfAddress">
    <xs:sequence>
      <xs:element name="Address" type="Address" nillable="false" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Address">
    <xs:attribute name="AddressId">
      <xs:annotation>
        <xs:documentation>Bank id of customer address. Used when address data is updated thru web services</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="50"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="AddressType" use="required">
      <xs:annotation>
        <xs:documentation>Type of the address</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:int">
          <xs:totalDigits value="10"/>
          <xs:enumeration value="0"/>
          <xs:enumeration value="1"/>
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
          <xs:enumeration value="4"/>
          <xs:enumeration value="5"/>
          <xs:enumeration value="6"/>
          <xs:enumeration value="7"/>
          <xs:enumeration value="8"/>
          <xs:enumeration value="9"/>
          <xs:enumeration value="10"/>
          <xs:enumeration value="11"/>
          <xs:enumeration value="12"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Name1" use="required">
      <xs:annotation>
        <xs:documentation>Person first name / company short name</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Name2" use="required">
      <xs:annotation>
        <xs:documentation>Person last name / company long name</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="AddressLine1" use="required">
      <xs:annotation>
        <xs:documentation>Address line 1</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="AddressLine2">
      <xs:annotation>
        <xs:documentation>Address line 2</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CityName" use="required">
      <xs:annotation>
        <xs:documentation>City name</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="50"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="PostalCode" use="required">
      <xs:annotation>
        <xs:documentation>Postal code</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CountryCode" use="required">
      <xs:annotation>
        <xs:documentation>Country code</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Email">
      <xs:annotation>
        <xs:documentation>Email</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="ContactPerson">
      <xs:annotation>
        <xs:documentation>Contact person for a company</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="UserData">
      <xs:annotation>
        <xs:documentation>Bank specific User Data</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="512"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="ArrayOfCard">
    <xs:sequence>
      <xs:element name="Card" type="Card" nillable="false" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Card">
    <xs:attribute name="PAN" use="required">
      <xs:annotation>
        <xs:documentation>Primary Account Number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="19"/>
          <xs:pattern value="[0-9]{6}[\*]+[0-9]{4}|[0-9]+"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Token" use="required">
      <xs:annotation>
        <xs:documentation>MPSI generated card identification number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="RequestId">
      <xs:annotation>
        <xs:documentation>Bank defined  identification number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="20"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CardAccount" use="required">
      <xs:annotation>
        <xs:documentation>Unique card account identification number</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="32"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CardType" use="required">
      <xs:annotation>
        <xs:documentation>Card product type</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CardSubType" use="required">
      <xs:annotation>
        <xs:documentation>Card sub-product type</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="3"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CardProfileId">
      <xs:annotation>
        <xs:documentation>Card fee profile Id</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="5"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="OwnershipType" use="required">
      <xs:annotation>
        <xs:documentation>Card ownership type</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="1"/>
          <xs:enumeration value="N">
            <xs:annotation>
              <xs:documentation>Basic(Primary)</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="P">
            <xs:annotation>
              <xs:documentation>Supplementary(Secondary)</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="B">
            <xs:annotation>
              <xs:documentation>Business</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BranchId">
      <xs:annotation>
        <xs:documentation>Id of branch which issued the card</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="10"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="CurrencyCode" type="xs:int" use="required">
      <xs:annotation>
        <xs:documentation>Currency of the credit card</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="CardStatus" use="required">
      <xs:annotation>
        <xs:documentation>Card lifecycle status</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="1"/>
          <xs:enumeration value="0"/>
          <xs:enumeration value="1"/>
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
          <xs:enumeration value="4"/>
          <xs:enumeration value="5"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BlockStatus" use="required">
      <xs:annotation>
        <xs:documentation>Card blocking status</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="1"/>
          <xs:enumeration value="0"/>
          <xs:enumeration value="1"/>
          <xs:enumeration value="2"/>
          <xs:enumeration value="3"/>
          <xs:enumeration value="4"/>
          <xs:enumeration value="5"/>
          <xs:enumeration value="6"/>
          <xs:enumeration value="7"/>
          <xs:enumeration value="A"/>
          <xs:enumeration value="B"/>
          <xs:enumeration value="C"/>
          <xs:enumeration value="D"/>
          <xs:enumeration value="E"/>
          <xs:enumeration value="F"/>
          <xs:enumeration value="G"/>
          <xs:enumeration value="H"/>
          <xs:enumeration value="I"/>
          <xs:enumeration value="J"/>
          <xs:enumeration value="K"/>
          <xs:enumeration value="M"/>
          <xs:enumeration value="L"/>
          <xs:enumeration value="N"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="BlockTimeStamp">
      <xs:annotation>
        <xs:documentation>Timestamp of card blocking. Format: YYYYMMDDHHMMSS</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="14"/>
          <xs:maxLength value="14"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="EmbossingLine1" use="required">
      <xs:annotation>
        <xs:documentation>Cardholder name and surname</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="50"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="EmbossingLine2">
      <xs:annotation>
        <xs:documentation>Company short name if applicable</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="50"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="ValidFrom" use="required">
      <xs:annotation>
        <xs:documentation>Card validity from. Format: YYYYMMDD</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="8"/>
          <xs:maxLength value="8"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="ValidThru" use="required">
      <xs:annotation>
